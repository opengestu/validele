{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "typescript-config/consistent-casing": "off",
    "button-type": "off",
    "manifest-app-name": "off",
    "apple-touch-icons": "off",
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "-webkit-overflow-scrolling"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]",
          "iframe[loading]"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ]
  }
}